// [문제 링크]: https://www.acmicpc.net/problem/11720

//  숫자의 합
​
import java.util.*;
import java.lang.*;
import java.io.*;
​
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int size = sc.nextInt();
        String str = sc.next();
        int sum = 0;
​
        for (int i = 0; i < size; i++) {
            sum += str.charAt(i) - '0';
        }
​
        System.out.println(sum);
​
    }
}